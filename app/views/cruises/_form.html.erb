<%= simple_form_for(@cruise) do |f| %>
  <%= f.input :title %>
  <%= f.input :boat_model %>
  <%= f.input :description%>
  <%= f.input :from%>
  <%= f.input :to%>
  <%= f.input :starting_date%>
  <%= f.input :ending_date%>
  <%= f.input :boat_age%>
  <%= f.input :capacity%>
  <%= f.input :price%>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "btn btn-primary"%>
<% end %>
